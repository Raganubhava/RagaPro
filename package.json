{
  "name": "ragapro",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean-install": "rm -rf node_modules && find . -name 'node_modules' -type d -prune -exec rm -rf '{}' + && rm -f pnpm-lock.yaml && pnpm install"
  },
  "devDependencies": {
    "@eslint/js": "^9.5.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.34.3",
    "prettier": "^3.3.2",
    "turbo": "^2.0.6",
    "typescript-eslint": "^8.0.0-alpha.34"
  },
  "packageManager": "pnpm@10.24.0",
  "pnpm": {
    "overrides": {
      "react": "18.3.1",
      "react-native-web": "0.21.2",
      "react-native-reanimated": "3.19.4"
    },
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
